{
    "$schema": "https://turborepo.org/schema.json",
    "tasks": {
      "init": {
        "cache": false,
        "outputs": []
      },
      "dev": {
        "persistent": true,
        "cache": false
      },
      "build": {
        "dependsOn": ["^build"],
        "outputs": ["dist/**"]
      }
    }
  }
  